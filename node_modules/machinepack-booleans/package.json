{
  "name": "machinepack-booleans",
  "version": "0.1.1",
  "description": "Work with true/false values and boolean operators like AND (&&), OR (||), NOT (!), and the ternary qualifier (?/:).",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "Booleans",
    "machines",
    "machinepack"
  ],
  "author": "The Treeline Company",
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.10.1",
    "machine": "^13.0.0-11"
  },
  "devDependencies": {
    "mocha": "^2.5.3",
    "test-machinepack-mocha": "^2.0.0"
  },
  "machinepack": {
    "friendlyName": "Booleans",
    "machineDir": "machines/",
    "machines": [
      "to-boolean",
      "is-between",
      "is-defined",
      "is-undefined",
      "is-equal",
      "is-not-equal",
      "is-greater-than",
      "is-less-than",
      "is-truthy",
      "is-falsy",
      "and",
      "or",
      "not",
      "is-booleanish"
    ]
  }
}
