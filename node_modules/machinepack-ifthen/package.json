{
  "name": "machinepack-ifthen",
  "version": "1.3.0",
  "description": "Machines for branching based on comparisons, validations, etc.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "If...Then",
    "machines",
    "machinepack"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "dependencies": {
    "browserify-transform-machinepack": "^1.0.2",
    "lodash": "^3.9.2",
    "machine": "^13.0.0-0"
  },
  "devDependencies": {
    "test-machinepack-mocha": "^2.1.1"
  },
  "machinepack": {
    "friendlyName": "If...Then",
    "machineDir": "machines/",
    "machines": [
      "if-then-finally",
      "if-then-finally-sync",
      "if-true",
      "if-false"
    ],
    "testsUrl": "https://travis-ci.org/"
  },
  "browserify": {
    "transform": [
      "browserify-transform-machinepack"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:treelinehq/machinepack-ifthen.git"
  }
}
