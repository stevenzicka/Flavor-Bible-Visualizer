{
  "name": "machinepack-util",
  "version": "7.0.0-2",
  "description": "Miscellaneous utilities for everyday tasks with arrays, dictionaries, strings, etc.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js && mocha tests/**"
  },
  "keywords": [
    "Utilities",
    "machines",
    "machinepack"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "browserify": {
    "transform": [
      "browserify-transform-machinepack"
    ]
  },
  "dependencies": {
    "async": "2.0.1",
    "browserify-transform-machinepack": "^1.0.3",
    "crypto-js": "3.1.6",
    "lodash": "3.10.1",
    "machine": "^13.0.0-11",
    "resolve": "1.1.7",
    "rttc": "^9.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:treelinehq/machinepack-util.git"
  },
  "devDependencies": {
    "mocha": "3.0.2",
    "test-machinepack-mocha": "^2.1.6"
  },
  "machinepack": {
    "friendlyName": "Util",
    "machineDir": "machines/",
    "machines": [
      "hash",
      "guarantee",
      "require",
      "construct",
      "whilst",
      "pretty-print",
      "get-calculated-value"
    ],
    "testsUrl": "https://travis-ci.org/treelinehq/machinepack-util"
  }
}
