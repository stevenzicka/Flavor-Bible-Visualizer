{
  "machine": "to-number",
  "expectations": [
    {
      "using": {
        "string": "0"
      },
      "outcome": "success",
      "returns": 0
    },
    {
      "using": {
        "string": "-5"
      },
      "outcome": "success",
      "returns": -5
    },
    {
      "using": {
        "string": "5"
      },
      "outcome": "success",
      "returns": 5
    },
    {
      "using": {
        "string": "5.5"
      },
      "outcome": "success",
      "returns": 5.5
    },
    {
      "using": {
        "string": "1.5e+50"
      },
      "outcome": "success",
      "returns": 1.5e+50
    },
    {
      "using": {
        "string": "1.5e-50"
      },
      "outcome": "success",
      "returns": 1.5e-50
    },
    {
      "using": {
        "string": "Infinity"
      },
      "outcome": "error"
    },
    {
      "using": {
        "string": "-Infinity"
      },
      "outcome": "error"
    },
    {
      "using": {
        "string": "abc"
      },
      "outcome": "error"
    },
    {
      "using": {
        "string": "\"true\""
      },
      "outcome": "error"
    },
    {
      "using": {
        "string": "\"false\""
      },
      "outcome": "error"
    }
  ]
}
