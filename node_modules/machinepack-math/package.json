{
  "name": "machinepack-math",
  "version": "1.2.0",
  "description": "Harness the power of math.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "Math",
    "machines",
    "machinepack"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "browserify-transform-machinepack": "^1.0.0",
    "lodash": "^3.8.0",
    "machine": "^13.0.0-11",
    "rttc": "^7.0.0"
  },
  "devDependencies": {
    "test-machinepack-mocha": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "browserify-transform-machinepack"
    ]
  },
  "machinepack": {
    "friendlyName": "Math",
    "machineDir": "machines/",
    "machines": [
      "add",
      "divide",
      "subtract",
      "multiply",
      "random",
      "e",
      "sqrt",
      "pi",
      "pow",
      "ln",
      "log",
      "ceil",
      "floor",
      "sum",
      "avg",
      "median",
      "min",
      "max",
      "round",
      "abs",
      "mod"
    ],
    "testsUrl": "https://travis-ci.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:treelinehq/machinepack-math.git"
  }
}
